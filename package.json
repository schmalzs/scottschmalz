    "build": "rimraf dist && run-p build:client build:server",
    "build:watch": "rimraf dist && run-p build:client:watch build:server:watch",
    "build:client": "rimraf dist/public && webpack",
    "build:client:watch": "nodemon --watch src/client --exec yarn build:client",
    "build:server": "rimraf dist/server && babel src/server -d dist/server",
    "build:server:watch": "nodemon --watch src/server --exec run-s build:server start",
    "lint": "eslint .",
    "heroku-postbuild": "yarn build",
    "start": "node dist/server",
